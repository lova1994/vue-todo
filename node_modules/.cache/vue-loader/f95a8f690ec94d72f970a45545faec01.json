div>\n  </div>\n</template>\n<script>\nexport default {\n  data: () => ({\n    title: \"Lovas Todos\",\n    todos: [\n      { item: \"Drink coffee\", isCompleted: false, id: Date.now()},\n      { item: \"Code\", isCompleted: false, id: Date.now() },\n      { item: \"Cry\", isCompleted: false, id: Date.now() },\n      { item: \"Repeat\", isCompleted: false, id: Date.now() }\n    ]\n  }),\n\n  computed: {},\n\n  methods: {\n    isDone() {\n      this.isDone = !this.isDone;\n    },\n\n    addTodo(event) {\n      const item = event.target.value;\n      this.todos.push({ item, isCompleted: false, id: Date.now() });\n      event.target.value = \"\";\n    },\n\n    removeTodo(id){\n      this.todos = this.todos.filter(todo => todo.id !== id)\n\n    }\n  }\n};\n</script>\n\n\n<style>\n.wrapper {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  color: #ffa2d0;\n}\n\n#app {\n  font-family: courier, monospace;\n}\n\nh1 {\n  font-family: \"press start 2p\";\n  font-size: 3rem;\n}\n\n.strikeThrough {\n  text-decoration: line-through;\n}\n\n#normal {\n  text-decoration: none;\n}\n\n.active {\n  background-color: #a2ffb4;\n  padding: 3%;\n}\n\n.strike {\n  text-decoration: line-through;\n  transition: 3s;\n}\n\nbody {\n  background-color: #222;\n}\n\nli:hover {\n  cursor: pointer;\n}\n\ninput {\n  background: #222;\n  border:none;\n  border-bottom: solid 2px #ffa2d0;\n  width: 10rem;\n  height: 2rem;\n  font-family: 'courier new';\n  font-size: 1.3rem;\n  color: #ffa2d0;\n}\n\nli {\n  font-size: 2rem;\n}\n\n::placeholder {\n  font-family: 'courier new';\n  font-size: 1.2rem;\n}\n\n.nes-btn {\n\n  border: none;\n  color: #ffa2d0;\n  font-size: 1rem;\n  background: #222;\n}\n\n\n.nes-btn:hover {\n  cursor:pointer;\n}\n</style>\n"]}]}